{
  "name": "react-calculator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android && react-native start",
    "android:release": "npx react-native run-android --variant=release",
    "android:deploy": "cd android && ./gradlew clean && ./gradlew :app:bundleRelease",
    "genkey": "keytool -genkeypair -v -keystore react-calculator-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000 && mv react-calculator-key.keystore android/app/",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@reduxjs/toolkit": "^1.8.3",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@redux-devtools/core": "^3.13.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.3",
    "@types/react-native-sqlite-storage": "^5.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
